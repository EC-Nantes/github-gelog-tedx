/**
 *
 * @author vdufo
 */
public class testWoE {

    /**
     *
     * @param args
     */
    public static void main(String args[]){
        System.setProperty("stdout.encoding", "UTF-8");
        
        World testWorld = new World();
        
        //test creerMondeAlea()
        System.out.println("Test creerMondeAlea() : ");
        testWorld.creerMondeAlea();
        testWorld.afficheWorld();
        
        System.out.println("Test pour la liste de position() : ");
        System.out.println(" ");
        System.out.println(testWorld.getPosition());
        

        //test deplace
        
        testWorld.getRobin().get(0).deplace(testWorld.getPosition());
        System.out.println(" ");
        System.out.println(testWorld.getPosition());
    }
}
