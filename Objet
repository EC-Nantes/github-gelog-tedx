/**
 *
 * @author rapha
 */
public class Objet {
    private int Pdurabilite;
    private boolean etat;
    private int niveau;
    
    public Objet(int Pdurabilite, boolean etat, int niveau){
        this.Pdurabilite=Pdurabilite;
        this.etat=etat;
        this.niveau=niveau;
    }

    public int getPdurabilite() {
        return Pdurabilite;
    }

    public void setPdurabilite(int Pdurabilite) {
        this.Pdurabilite = Pdurabilite;
    }

    public boolean isEtat() {
        return etat;
    }

    public void setEtat(boolean etat) {
        this.etat = etat;
    }

    public int getNiveau() {
        return niveau;
    }

    public void setNiveau(int niveau) {
        this.niveau = niveau;
    }

}
