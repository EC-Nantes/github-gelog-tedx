public class Paysan extends Personnage{

    
    public Paysan(String n, int pV, int dA, int pPar, int paPar,int paAtt, int dMax, Point2D p){
        super(n, pV, dA, pPar, paPar, paAtt, dMax, p);
  
    }
    public Paysan(Paysan pa){
        super((Personnage)pa);   
    }
    public Paysan(){
        super("paysan", 10, 10, 10, 10, 20, 30, new Point2D(5,3));
        
        
    }
}
