public class Archer extends Personnage{
    private int nbFleches;
    
    public Archer(String n, int pV, int dA, int pPar, int paPar,int paAtt, int dMax, Point2D p,int nbFleches){
        super(n, pV, dA, pPar, paPar, paAtt, dMax, p);
        this.nbFleches=nbFleches;
        
        
    }
    
}
