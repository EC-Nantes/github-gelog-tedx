package org.centrale.objet.WoE;

/**
 *
 * @author vdufo
 */
public abstract class Nourriture extends Objet {
    private boolean type; // true = Bonus, false = Malus
    
    public Nourriture(boolean etat, Point2D pos, boolean t) {
        super(1, etat, 1, pos);
        this.type = t;
    }
    
    public boolean getType(){
        return this.type;
    }
    
    public void setType(boolean t){
        this.type = t;
    }
    
    public String nomClasse(){
        return "nourriture";
    }
    
    
}
